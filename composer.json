{
    "name": "ictact-coop/eco-yongin",
    "description": "용인환경정의 웹사이트",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Yeongtaek, Hong",
            "role": "Developer",
            "email": "mozodev@users.noreply.github.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-project-message": "^8.8",
        "drupal/core-recommended": "^8.8",
        "drupal/asset_injector": "*",
        "drupal/better_exposed_filters": "*",
        "drupal/ckeditor_font": "*",
        "drupal/ckeditor_uploadimage": "*",
        "drupal/ckeditor_widgets": "*",
        "drupal/colorbutton": "*",
        "drupal/core": "^8.6",
        "drupal/ctools": "*",
        "drupal/entity": "*",
        "drupal/entity_browser": "*",
        "drupal/entity_reference_revisions": "*",
        "drupal/flysystem": "*",
        "drupal/flysystem_s3": "*",
        "drupal/google_analytics": "*",
        "drupal/masquerade": "*",
        "drupal/media_entity": "*",
        "drupal/media_entity_image": "*",
        "drupal/node_view_permissions": "*",
        "drupal/panelbutton": "*",
        "drupal/paragraphs": "*",
        "drupal/smtp": "*",
        "drupal/superfish": "*",
        "drupal/video_embed_field": "*",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "process-timeout":0
    },
    "autoload": {
        "files": ["load.environment.php"]
    },
    "scripts": {
        "start": ["drush rs /admin"]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "patches": {
            "drupal/core": {
                "2997982-45": "https://www.drupal.org/files/issues/2018-09-12/2997982-45.patch",
                "truncate_taxonomy_term__parent_table": "https://www.drupal.org/files/issues/2018-10-17/2997982_truncate_taxonomy_term__parent_table.patch"
            }
        }
    }
}
